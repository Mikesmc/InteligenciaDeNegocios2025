                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
      dd.date_key
    , dc.customer_key
    , dp.product_key
    , de.employee_key
    , ds.shipper_key
    , sv.Ventas_NOrden       AS order_number
    , sv.Ventas_Unidades     AS order_qty
    , sv.Ventas_PUnitario    AS unit_price
    , sv.Ventas_Descuento    AS discount
FROM Stage_Northwind.dbo.Stage_Ventas AS sv
JOIN Datamart_Northwind.dbo.dim_customer  AS dc
      ON sv.Cliente_Codigo   = dc.customer_nk
     AND dc.is_current       = 1          -- ⬅️ muy importante
JOIN Datamart_Northwind.dbo.dim_employee  AS de
      ON sv.Empleado_Codigo  = de.employeeid_nk
JOIN Datamart_Northwind.dbo.dim_product   AS dp
      ON sv.Producto_Codigo  = dp.productid_nk
JOIN Datamart_Northwind.dbo.dim_date      AS dd
      ON dd.[date]           = CONVERT(date, sv.Ventas_OrderDate)  -- ⬅️ quitar hora
JOIN Datamart_Northwind.dbo.dim_shipper   AS ds
      ON ds.shipperid_nk     = sv.ShipperID;</property>
