                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
      dd.date_key                         AS order_date_key
    , dc.customer_key
    , dp.product_key
    , de.employee_key
    , ds.shipper_key
    , sv.Ventas_NOrden                    AS order_number
    , sv.Ventas_Unidades                  AS order_qty
    , CAST(sv.Ventas_PUnitario AS DECIMAL(19,4)) AS unit_price
    , CAST(sv.Ventas_Descuento AS DECIMAL(5,2))  AS discount
FROM STAGE_NORTHWIND.dbo.Stage_Ventas AS sv
JOIN Datamart_Northwind.dbo.dim_customer  AS dc
    ON sv.Cliente_Codigo       = dc.customerid_nk
   AND dc.is_current           = 1            -- 🔹 solo la versión actual del cliente
JOIN Datamart_Northwind.dbo.dim_employee  AS de
    ON sv.Empleado_Codigo      = de.employeeid_nk
JOIN Datamart_Northwind.dbo.dim_product   AS dp
    ON sv.Producto_Codigo      = dp.productid_nk
JOIN Datamart_Northwind.dbo.dim_shipper   AS ds
    ON sv.Transportista_codigo = ds.shipperid_nk
JOIN Datamart_Northwind.dbo.dim_date      AS dd
    ON dd.[date] = CAST(sv.Ventas_OrderDate AS date)
WHERE 
    -- 1) Fechas &gt;= a la máxima que ya tienes
    CAST(sv.Ventas_OrderDate AS date) &gt;= COALESCE(
          (SELECT MAX(d.[date])
           FROM Datamart_Northwind.dbo.fact_sales fs
           JOIN Datamart_Northwind.dbo.dim_date d 
                ON fs.order_date_key = d.date_key),
          '19000101'
      )	
    -- 2) Evitar duplicar orders que ya estén en la fact
    AND NOT EXISTS (
        SELECT 1
        FROM Datamart_Northwind.dbo.fact_sales fs
        WHERE fs.order_number = sv.Ventas_NOrden
    )
ORDER BY 
      dc.customer_key
    , de.employee_key
    , dp.product_key
    , ds.shipper_key
    , dd.date_key
    , sv.Ventas_NOrden;</property>
